{
  "presets": [
    [
      "env", {
        "modules": "commonjs",
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime"
  ]
}
